<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24"
    android:tint="?attr/colorControlNormal">

    <!--
        Cuerpo principal de la RAM con muesca integrada.
        Path explanation:
             M2,7       Move to top-left (x=2, y=7)
             h20        Horizontal line to top-right (x=22, y=7)
             v10        Vertical line down to bottom-right (x=22, y=17)
             h-8        Horizontal line left (to x=14, y=17) - Start of right part of notch base
             v-1.5      Vertical line up (to x=14, y=15.5) - Right side of notch
             h-4        Horizontal line left (to x=10, y=15.5) - Top of notch
             v1.5       Vertical line down (to x=10, y=17) - Left side of notch
             h-8        Horizontal line left (to x=2, y=17) - To bottom-left
             Z          Close path (back to 2,7, creating the left edge)
    -->
    <path
        android:fillColor="@android:color/white"
        android:pathData="M2,7 h20 v10 h-8 v-1.5 h-4 v1.5 h-8 Z"/>

    <!-- Pequeños chips simulados en la RAM.
         Se dibujan sobre el cuerpo principal. Como tienen el mismo fillColor,
         se fusionarán visualmente con el cuerpo cuando se aplique el tint,
         formando parte de la silueta general del ícono tintado. -->
    <path
        android:fillColor="@android:color/white"
        android:pathData="M4,9h2v2H4z"/>
    <path
        android:fillColor="@android:color/white"
        android:pathData="M7,9h2v2H7z"/>
    <path
        android:fillColor="@android:color/white"
        android:pathData="M10,9h2v2H10z"/>
    <path
        android:fillColor="@android:color/white"
        android:pathData="M13,9h2v2H13z"/>
    <path
        android:fillColor="@android:color/white"
        android:pathData="M16,9h2v2H16z"/>
    <path
        android:fillColor="@android:color/white"
        android:pathData="M19,9h2v2H19z"/>
</vector>